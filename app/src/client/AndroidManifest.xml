<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="in.mobifirst.tagtree">

    <application
        android:name=".application.IQClientApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true">
        <activity
            android:name=".tokens.TokensActivity"
            android:theme="@style/AppTheme.Background"></activity>
        <activity
            android:name=".activity.WelcomeActivity"
            android:theme="@style/AppTheme.NoTitleBar.Fullscreen"></activity>

        <service
            android:name=".MyFirebaseMessagingService">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT"/>
            </intent-filter>
        </service>

        <meta-data
            android:name="io.fabric.ApiKey"
            android:value="da7a69f0dee3ffe21c69c8e4aff9bdff1db53059"
            />
        
        <service
            android:name=".MyFirebaseInstanceIdService">
            <intent-filter>
                <action android:name="com.google.firebase.INSTANCE_ID_EVENT"/>
            </intent-filter>
        </service>
    </application>

</manifest>