<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="in.mobifirst.tagtree">

    <uses-permission android:name="android.permission.VIBRATE" />

    <application
        android:name=".application.IQClientApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true">
        <activity
            android:name=".tokens.LandingActivity"
            android:screenOrientation="portrait"></activity>
        <activity
            android:name=".activity.RequestPermissionsActivity"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.Background" />
        <activity
            android:name=".authentication.digits.DigitsSignInActivity"
            android:screenOrientation="portrait" />

        <activity
            android:name=".requesttoken.RequestTokenActivity"
            android:screenOrientation="portrait"></activity>

        <service android:name=".MyFirebaseMessagingService">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT" />
            </intent-filter>
        </service>

        <service android:name=".MyFirebaseInstanceIdService">
            <intent-filter>
                <action android:name="com.google.firebase.INSTANCE_ID_EVENT" />
            </intent-filter>
        </service>

        <meta-data
            android:name="io.fabric.ApiKey"
            android:value="da7a69f0dee3ffe21c69c8e4aff9bdff1db53059" />
    </application>

</manifest>